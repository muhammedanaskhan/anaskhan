{
  "name": "personal-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nx exec -- next dev",
    "build": "nx exec -- next build",
    "start": "nx exec -- next start",
    "lint": "nx exec -- next lint"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "next": "13.1.6",
    "react": "17.0.2",
    "react-animated-cursor": "^2.2.0",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "nx": "15.6.3",
    "@nrwl/nx-cloud": "latest"
  },
  "nx": {
    "targets": {
      "dev": {
        "outputs": [
          "{projectRoot}/y"
        ]
      },
      "build": {
        "outputs": [
          "{projectRoot}/y"
        ]
      },
      "start": {
        "outputs": [
          "{projectRoot}/y"
        ]
      },
      "lint": {
        "outputs": [
          "{projectRoot}/y"
        ]
      }
    }
  }
}
